  <style id="antiClickjack">body{display:none !important;}</style>
       <script type="text/javascript">
     if (self === top) {
         var antiClickjack = document.getElementById("antiClickjack");
         antiClickjack.parentNode.removeChild(antiClickjack);
     } else {
         top.location = self.location;
     }
      </script>

  <body>
  	 <div id="answers" style="display:none;">
      <%= answers %>
  	 </div>

     <div id="questions" style="display:none;">
      <%= questions %>
     </div>

  	
  <div class="container text-center">
    <div class="quizbox">
      <h2 class="mt-5 mb-5 font-weight-bold">Bonne chance et amusez vous bien.</h2>
      <p class="description">Voici les questions :
      
      <br>
      <br>
      <span class="q" id="question">Est ce que vous etes pret ?</span>
      

      <div class="container-fluid">
        <div class="code-wrapper">
        </div>
        <div class="button-container pb-5">
  		
          <h5 class="m-4">
  			<b id="current">0</b> sur <b>15</b>
  		</h5>
          <div class="options mb-4"></div>
         
         <div id="proceed" class="btn btn-primary w-50 rounded-0 border-0 p-2">Next</div>
        </div>
      </div>
    </div>

    <div class="final-message p-2 p-md-5">
      
      <h3 class="mt-3 font-weight-bold">Vous avez <b id="guessed">0</b> bonnes r√©ponses sur <b>15</b> questions !</h3>
      <h6 class="mt-3 text-muted mb-4 text-uppercase">Vous ferez mieux la prochaine fois.</h6>
      <a href ="/">Menu</a>
    </div>
    
  </div>

  </body>
